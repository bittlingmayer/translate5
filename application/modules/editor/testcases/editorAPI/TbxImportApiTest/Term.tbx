						</termGrp>
						
						
						<!-- this is not saved in the table  -->
						<transacGrp>
							<!-- this is saved in the lek attributes, add this parrentId to the date and transacNote  -->
							<transac>creation </transac>
							<!-- this here have only parrent id  -->
							<date>2018-01-12 </date>
							<!-- this here have only parrent id  -->
							<transacNote type="responsiblePerson">Roland Dietrich </transacNote>
						</transacGrp>
						<transacGrp>
							<transac>modification </transac>
							<date>2018-01-12 </date>
							<transacNote type="responsiblePerson">Roland Dietrich </transacNote>
						</transacGrp>
					</ntig>
					<ntig>
